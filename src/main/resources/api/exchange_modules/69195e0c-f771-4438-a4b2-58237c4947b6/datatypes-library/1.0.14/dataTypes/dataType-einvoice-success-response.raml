#%RAML 1.0 DataType
properties:
  statusCode:
    description: code to specify the status of publishing file in external system
    example: 200
    type: number
  statusMessage:
    description: Invoice Acknowledgment Processed Successfully
    type: string
    example: "Processing of File is in progress"
  correlationID:
    description: The Correlation ID of the request
    type: string
    example: "28393680-805f-11ed-a40a-00059a3c7a00"